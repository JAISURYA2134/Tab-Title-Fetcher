{
    "manifest_version": 3,
    "name": "Tab Title Fetcher",
    "version": "1.0",
    "description": "Fetches and displays the title of the current tab.",
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "icons":{
      "128":"logo.png"
    }
  }